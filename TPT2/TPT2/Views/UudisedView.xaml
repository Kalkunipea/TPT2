<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TPT2.Views.UudistePage"
             xmlns:views="clr-namespace:TPT2.Views"
             xmlns:viewModels="clr-namespace:TPT2.ViewModels;assembly=TPT2"
             Title="{Binding Title}">
    <ContentPage.BindingContext>
        <viewModels:UudisedViewModel/>
    </ContentPage.BindingContext>
    <ContentPage.Content>

        <StackLayout>
            <WebView Source="https://www.tptlive.ee/category/uudised/"
                     HeightRequest="800"
                     WidthRequest="300" />

        </StackLayout>
    </ContentPage.Content>
</ContentPage>